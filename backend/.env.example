# Configurações do servidor
PORT=3001
NODE_ENV=development

# Configurações do banco de dados
DB_HOST=localhost
DB_PORT=5432
DB_NAME=speedfunnels
DB_USER=postgres
DB_PASSWORD=your_password

# Configurações do Redis
REDIS_URL=redis://localhost:6379

# Configurações de autenticação
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRATION=24h

# Configurações do Meta Ads API
META_ACCESS_TOKEN=your_meta_access_token
META_AD_ACCOUNT_ID=act_your_ad_account_id

# Configurações do Google Analytics
GA_VIEW_ID=your_ga_view_id
GA_CLIENT_EMAIL=your_service_account_email@example.com
GA_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour private key content here\n-----END PRIVATE KEY-----"
